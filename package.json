{
  "name": "v-emotion",
  "version": "2.0.0",
  "description": "A VueJs implementation of Emotion.",
  "author": "Javier Alonso Shannon",
  "license": "MIT",
  "homepage": "https://github.com/javialon26/v-emotion#readme",
  "repository": {
    "type": "git",
    "url": "github:javialon26/v-emotion"
  },
  "keywords": [
    "vuejs",
    "vue",
    "emotion",
    "css-in-js",
    "css"
  ],
  "scripts": {
    "clean": "rimraf lib es",
    "build": "npm run build:commonjs && npm run build:es",
    "build:es": "BABEL_ENV=es babel src -d es",
    "build:commonjs": "BABEL_ENV=commonjs babel src -d lib",
    "lint": "eslint --ignore-path .gitignore src",
    "lint:fix": "eslint --ignore-path .gitignore src --fix",
    "prepare": "npm run clean && npm run build"
  },
  "dependencies": {
    "emotion": "^10.0.7"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.2",
    "@babel/preset-env": "^7.3.1",
    "eslint": "^5.13.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.16.0",
    "rimraf": "^2.6.3"
  },
  "main": "lib/index.js",
  "module": "es/index.js",
  "jsnext:main": "es/index.js",
  "files": [
    "lib",
    "es"
  ]
}
