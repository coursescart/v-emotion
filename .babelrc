{
  "env": {
    "commonjs": {
      "presets": [
        ["@babel/preset-env", {
          "useBuiltIns": false
        }]
      ]
    },
    "es": {
      "presets": [
        ["@babel/preset-env", {
          "useBuiltIns": false,
          "modules": false
        }]
      ]
    }
  },
  "presets": [
    "@babel/preset-env"
  ],
  "plugins": [
    "@babel/plugin-proposal-object-rest-spread"
  ]
}
